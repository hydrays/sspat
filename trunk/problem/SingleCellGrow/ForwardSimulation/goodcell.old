'goodcell' <- function(T, p){
	   x <- matrix(nrow = 2^T, ncol = 1)
	   x[] <- -1
	   x[1,1] <- 1
	   ngood_new <- 1
	   nbad <- 0
	   for (i in seq(T)){
	       ngood <- ngood_new
	       r <- runif(ngood)
	       for (j in seq(ngood)){
	       	   if ( r[j] > p ){ 
		      ngood_new <- ngood_new + 1
		      x[ngood_new,1] <- 1
		   else
		      nbad <- nbad + 1
		         
	       }    
	       cat(i, x[i],'\n')
	   }
	   return(x)
}     
	       